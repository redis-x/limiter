{
	"name": "@redis-x/limiter",
	"version": "0.1.0-beta.2",
	"description": "",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "src/main.js",
	"types": "dist/types/main.d.ts",
	"exports": {
		".": {
			"import": "./src/main.js",
			"require": "./dist/main.cjs"
		}
	},
	"engines": {
		"node": ">=14"
	},
	"dependencies": {
		"valibot": "^0.37.0"
	},
	"peerDependencies": {
		"@redis-x/client": "^0.2.0-beta.6",
		"redis": "^4.6"
	},
	"devDependencies": {
		"@kirick/eslint-config": "^0.1.14",
		"@types/node": "^20.14.14",
		"eslint": "9.8.0",
		"typescript": "5.5.4",
		"vitest": "1.3.1"
	},
	"scripts": {
		"build": "bun run build:types && bun run build:cjs && bun run build:cjs:tests",
		"build:cjs": "bunx esbuild --bundle --platform=node --format=cjs --packages=external --outfile=dist/main.cjs src/main.js",
		"build:cjs:tests": "bunx esbuild src/limiter.test.js --outfile=dist/main.test.cjs --format=cjs && sed -i 's|require(\"./main.js\")|require(\"./main.cjs\")|g' dist/main.test.cjs",
		"build:types": "rm -rf dist/esm dist/types ; bunx tsc --skipLibCheck --emitDeclarationOnly && rm dist/**/*.test.d.ts",
		"lint": "eslint . && tsc --skipLibCheck --noemit",
		"publish:npm": "bun run build && bun run lint && bun run test && npm publish",
		"redis:up": "docker ps | grep test-redis >/dev/null || docker run --rm -d -p 16379:6379 --name test-redis redis:7-alpine",
		"test": "export REDIS_PORT=16379 ; bun run redis:up && npm run test:vitest && bun test --coverage",
		"test:vitest": "vitest run --no-file-parallelism"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/redis-x/limiter.git"
	},
	"keywords": [],
	"author": "Daniil Kirichenko (https://twitter.com/kirickme)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/redis-x/limiter/issues"
	},
	"homepage": "https://github.com/redis-x/limiter#readme"
}
